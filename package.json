{
  "name": "boilerplate-svelte",
  "version": "1.0.0",
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/plugin-transform-runtime": "^7.7.4",
    "@babel/preset-env": "^7.7.1",
    "@storybook/addon-actions": "^5.2.6",
    "@storybook/addon-links": "^5.2.6",
    "@storybook/addons": "^5.2.6",
    "@storybook/svelte": "^5.2.6",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/svelte": "^1.9.1",
    "@typescript-eslint/eslint-plugin": "^2.9.0",
    "@typescript-eslint/parser": "^2.9.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "cross-env": "^6.0.3",
    "css-loader": "^3.2.0",
    "eslint": "^6.7.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-svelte3": "^2.7.3",
    "file-loader": "^5.0.2",
    "html-webpack-inline-source-plugin": "0.0.10",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "jest-dom": "^4.0.0",
    "jest-transform-svelte": "^2.1.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.13.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "prettier-plugin-svelte": "^0.7.0",
    "serve": "^11.0.0",
    "style-loader": "^1.0.0",
    "svelte": "^3.15.0",
    "svelte-loader": "^2.13.6",
    "svelte-preprocess": "^3.2.6",
    "svelte-test": "^0.4.0",
    "typescript": "^3.7.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "jest": {
    "transform": {
      "^.+\\.svelte$": "jest-transform-svelte"
    },
    "moduleFileExtensions": [
      "js",
      "svelte"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint:eslint && yarn lint:prettier"
    }
  },
  "setupFilesAfterEnv": [
    "@testing-library/jest-dom/extend-expect"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack",
    "dev": "webpack-dev-server --content-base public",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "format": "npm-run-all -c format:*",
    "format:eslint": "eslint --fix '**/*.{js,ts}'",
    "format:prettier": "prettier --write '**/*.{js,ts,svelte,json,md,css}'",
    "lint": "npm-run-all -c lint:*",
    "lint:eslint": "eslint '**/*.{js,ts}'",
    "lint:prettier": "prettier --check '**/*.{js,ts,json,md,css}'",
    "test": "jest src --passWithNoTests --coverage",
    "test:watch": "npm run test -- --watch"
  },
  "dependencies": {
    "eslint-config-airbnb": "^18.0.1",
    "svelte-routing": "^1.4.0"
  },
  "description": "Boilerplate Svelte Javascript Framework",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agusID/boilerplate-svelte.git"
  },
  "author": "Dwi Agustianto <dwi.agustianto@ralali.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/agusID/boilerplate-svelte/issues"
  },
  "homepage": "https://github.com/agusID/boilerplate-svelte#readme",
  "directories": {
    "doc": "docs"
  },
  "keywords": [
    "Boilerplate",
    "Svelte",
    "Javascript"
  ]
}
